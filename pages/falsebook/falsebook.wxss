/* pages/falsebook/falsebook.wxss */
.page-container {
  position: fixed;
  height: 100vh;
  width: 100vw;
  overflow: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  background: #f3f6fb;
}
.empty-container {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.loader {
  text-align: center;
}
